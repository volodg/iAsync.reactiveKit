CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/ReactiveKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Result.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/iAsync_async.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/iAsync_reactiveKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/iAsync_utils.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "ReactiveKit" -framework "Result" -framework "iAsync_async" -framework "iAsync_reactiveKit" -framework "iAsync_utils"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-iAsync.reactiveKitApp
PODS_ROOT = ${SRCROOT}/Pods
SWIFT_INCLUDE_PATHS = ${PODS_ROOT}/SysXattr